   <define name="mei_att.lyricsorder.attributes">
      <ref name="mei_att.lyricsorder.attribute.row"/>
   </define>
   <define name="mei_att.lyricsorder.attribute.row">
      <optional>
         <attribute name="row" ns="http://github.com/annplaksin/mensural_sourceEncoding">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Counts rows of lyrics within a system.</a:documentation>
            <data type="positiveInteger"/>
         </attribute>
      </optional>
   </define>
   <define name="mei_att.lyricstype.attributes">
      <ref name="mei_att.lyricstype.attribute.lyric-type"/>
   </define>
   <define name="mei_att.lyricstype.attribute.lyric-type">
      <optional>
         <attribute name="lyric-type"
                    ns="http://github.com/annplaksin/mensural_sourceEncoding">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Classifies the type of a lyric portion in a source.</a:documentation>
            <choice>
               <value>full</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">full lyrics below stave</a:documentation>
               <value>incipit</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">only text incipit given</a:documentation>
               <value>additional</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">additional lyrics independent of staves</a:documentation>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="mei_att.accid.pos.attributes">
      <ref name="mei_att.accid.pos.attribute.accidPos"/>
   </define>
   <define name="mei_att.accid.pos.attribute.accidPos">
      <optional>
         <attribute name="accidPos"
                    ns="http://github.com/annplaksin/mensural_sourceEncoding">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Relative position of an accidental to following note. Exact position could be given with other attributes.</a:documentation>
            <choice>
               <value>before</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Accidental is in front of the note</a:documentation>
               <value>above</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Accidental is above the note</a:documentation>
               <value>below</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Accidental is below the note</a:documentation>
            </choice>
         </attribute>
      </optional>
   </define>
   <start>
      <choice>
         <ref name="mei_mei"/>
         <ref name="mei_music"/>
      </choice>
   </start>
   <ns xmlns="http://purl.oclc.org/dsdl/schematron"
       prefix="mei"
       uri="http://www.music-encoding.org/ns/mei"/>
   <ns xmlns="http://purl.oclc.org/dsdl/schematron"
       prefix="xlink"
       uri="http://www.w3.org/1999/xlink"/>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mei-accidAncestors-constraint-rule-76">
      <sch:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="mei:accid">
         <sch:assert test="not(parent::mei:note)">An accidental is not allowed as child element
                of a note within mensural source encoding.</sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mei-accidAttributes-constraint-rule-77">
      <sch:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="mei:accid">
         <sch:assert test="@accid and @ploc and @oloc">
                Accid needs @accid, @ploc, and @oloc.
              </sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mei-proportAtts-constraint-rule-78">
      <sch:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="mei:proport">
         <sch:assert test="@altsym and @num and @numbase and @*[local-name() = 'affected']">
                Proport needs @num, @numbase, @altsym, and @mse:affects.
              </sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mei-mensurAtts-constraint-rule-79">
      <sch:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="mei:mensur">
         <sch:assert test="@altsym and @tempus and @prolatio">
                Mensur needs @altsym, @tempus, and @prolatio.
              </sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mei-ligatureForm-constraint-rule-80">
      <sch:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="mei:ligature">
         <sch:assert test="@form">Ligature needs a form attribute.</sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mei-punctum-constraint-rule-81">
      <sch:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="mei:dot">
         <sch:assert test="not(ancestor::mei:note)">A dot cannot be a descendant element of a
                note.</sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mei-dotAfterNote-constraint-rule-82">
      <sch:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="mei:note[following-sibling::*[1]/local-name() = 'dot']">
         <sch:assert test="@num  and @numbase">
                Notes with a following dot need @num and @numbase.
              </sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mei-custosPos-constraint-rule-83">
      <sch:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="mei:custos">
         <sch:assert test="@pname  and @oct">
                Custos need @pname and @oct.
              </sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mei-keyAccidPos-constraint-rule-84">
      <sch:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="mei:keyAccid">
         <sch:assert test="@pname  and @oct">
                keyAccid need @pname and @oct.
              </sch:assert>
      </sch:rule>
   </pattern>
</grammar>
